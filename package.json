{
  "name": "nextsetup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "devclean": "npm cache clean --force && del /q /s .next && next dev",
    "devserver": "nodemon src/server/index.ts",
    "previewserver": "next build && tsc -p src/server/tsconfig.json && cross-env NODE_ENV=production node dist/index.js",
    "build": "next build",
    "buildserver": "next build && tsc -p src/server/tsconfig.json",
    "start": "next start",
    "startserver": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "preview": "next build && next start",
    "prepare": "husky install",
    "json-server": "npx json-server --watch src/data/db.json --port 8080"
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.8",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "discord.js": "^13.8.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "gsap": "^3.10.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.1",
    "next": "^12.2.0",
    "next-transpile-modules": "^9.0.0",
    "nodemailer": "^6.7.6",
    "random-words": "^1.2.0",
    "react": "17.0.2",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "17.0.2",
    "react-donut-chart": "^1.3.0",
    "react-google-recaptcha": "^2.1.0",
    "react-mount-animation": "^0.0.11",
    "react-slick": "^0.29.0",
    "sharp": "^0.30.4",
    "vanilla-tilt": "^1.7.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.31",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemon": "^1.19.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-slick": "^0.23.8",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "autoprefixer": "^10.4.6",
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-validate-jsx-nesting": "^0.0.8",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.18",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.4",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|scss|md|mdx)": [
      "prettier --write"
    ]
  }
}
